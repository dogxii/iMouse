*.xcworkspace/xcuserdata/

# Xcode 构建产物
build/
DerivedData/

# Swift Package Manager
.build/
.swiftpm/
Package.resolved

# CocoaPods（如果将来使用）
Pods/

# Carthage（如果将来使用）
Carthage/Build/
Carthage/Checkouts/

# macOS 系统文件
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
Icon?

